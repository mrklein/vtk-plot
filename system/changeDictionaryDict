FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    object      changeDictionaryDict;
}

dictionaryReplacement
{
    boundary
    {
        walls
        {
            type            wall;
        }

        cylinder
        {
            type            wall;
        }

        front-n-back
        {
            type            empty;
        }
    }
}

